; ModuleID = 'main'

@.str = private constant [9 x i8] c"B is %d\0A\00"

declare i32 @printf(i8*, ...)

define internal i32 @main() {
entry:
  %a = alloca i32
  %0 = load i32, i32* %a
  store i32 10, i32* %a
  %b = alloca i32
  %1 = load i32, i32* %b
  %2 = load i32, i32* %a
  %3 = load i32, i32* %a
  %4 = add i32 %3, 20
  store i32 %4, i32* %b
  %5 = load i32, i32* %b
  %6 = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([9 x i8], [9 x i8]* @.str, i32 0, i32 0), i32 %5)
  ret i32 0
}
